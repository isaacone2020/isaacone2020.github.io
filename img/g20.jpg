<script>
    $(document).ready(function () {
        //alert('hi');
        var showChar = 150;
        var ellipsestext = "";
        var moretext = "<p style='font-size:14px; text-transform:lowercase; font-weight:100;'>Read More</p>";
        var lesstext = "<p style='font-size:14px; text-transform:lowercase; font-weight:100;'>Read Less</p>";
        //if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            $('.more1').each(function () {
                var content = $(this).html();

                if (content.length > showChar) {

                    var c = content.substr(0, showChar);
                    var h = content.substr(showChar, content.length - showChar);
                    var html = c + '<span class="moreelipses">' + ellipsestext + '</span><span class="morecontent"><span id="moore">' + h + '</span><a href="" class="morelink">' + moretext + '</a></span>';
                    /* var html = c +'<span class="morecontent right"><span>'+ h +'</span><a href="" class="morelink right"  >' + moretext + '</a></span>';*/

                    $(this).html(html);
                }

            });
        //}

        $(".morelink").click(function () {
            if ($(this).hasClass("less")) {
                $(this).removeClass("less");
                $(this).html(moretext);
            } else {
                $(this).addClass("less");
                $(this).html(lesstext);
            }
            $(this).parent().prev().toggle();
            $(this).prev().toggle();
            return false;
        });

    });

</script>

